<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CodesPostaux" table="Codes_Postaux">
    <id name="idcodepostal" type="integer" column="idCodePostal">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomville" type="string" column="NomVille" length="45" nullable="true"/>
    <field name="codepostal" type="string" column="CodePostal" length="20" nullable="true"/>
    <many-to-one field="idadresse" target-entity="Adresse">
      <join-columns>
        <join-column name="idAdresse" referenced-column-name="idAdresse"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
