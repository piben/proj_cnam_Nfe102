{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CategorieProduit list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Datecreatecat</th>
                <th>Dateupdatecat</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('categorieproduit_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.type }}</td>
                <td>{% if entity.datecreatecat %}{{ entity.datecreatecat|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.dateupdatecat %}{{ entity.dateupdatecat|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('categorieproduit_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('categorieproduit_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('categorieproduit_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
